pipeline {
  agent none
  stages {
    stage('Build') {
      steps {
       sh ' mnv clean install package'
    }
steps{
sh 'class HelloWorld {
    public static void main(String[] args) {
        System.out.println("Hello, World!"); 
    }
}
}
      }
    }
    stage('Front-end') {
      agent {
        docker { image 'node:16-alpine' }
      }
      steps {
        sh 'node --version'
      }
    }
  }
}
